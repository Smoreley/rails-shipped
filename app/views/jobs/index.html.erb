<h1>Jobs Index Page</h1>
<br>

<% @jobs.each do |job| %>
    <!--VALUES-->
    <%= job.id %>.
    <%= job.name %>
    <%= job.description %>
    <%= job.origin %>
    <%= job.destination %>
    <%= job.cost %>
    <%= job.cc %>
    <br>

    <!-- LINKS -->
    <%= link_to 'Show', user_path(user.id) %>
    <%= link_to 'Edit', user_path(user.id)+"/edit" %>
    <%= link_to 'Destroy',  { action: :destroy, id: user.id }, method: :delete %>
<% end  %>