<h1>Users Index Page</h1>
<br>

<% @users.each do |user| %>
    <div class="panel panel-default">
        <div class="panel-body">
        <!--VALUES-->
        <%= user.id %>.
        <%= user.email %>
        <br>

        <!-- LINKS -->
        <%= link_to 'Show', user_path(user.id) %>
        <%= link_to 'Edit', user_path(user.id)+"/edit" %>
        <%= link_to 'Destroy',  { action: :destroy, id: user.id }, method: :delete %>
        </div>
    </div>
<% end  %>

<div class="row">
    <div class="col-sm-12">
        <%= button_to 'new', new_user_path, class: "btn btn-primary", :method => "get" %>
    </div>
</div>